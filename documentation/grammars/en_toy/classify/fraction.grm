import '../byte.grm' as b;
import '../util.grm' as u;

d = b.kDigit;
q = u.q;

#
# 1/2
# 
# into
#
# fraction { numerator: 1 denominator: 2 }
#

fraction =
  u.I["fraction { "]
  u.I["numerator: " q]
  d+
  u.I[q]
  u.D["/"]
  u.I["denominator: " q]
  d+
  u.I[q]
  u.I[" }"]
;

export FRACTION = Optimize[fraction];
