# Combines all of the semiotic classes together.

export ALL = Optimize[
 D.DECIMAL |  (d.DATE <-2.0>)| M.MEASURE | m.MONEY | n.CARDINAL_MARKUP | n.ORDINAL_MARKUP | t.TIME | v.VERBATIM];

