decades =   ("00" : "hundreds")
          | ("10" : "tens")
          | ("20" : "twenties")
          | ("30" : "thirties")
          | ("40" : "forties")
          | ("50" : "fifties")
          | ("60" : "sixties")
          | ("70" : "seventies")
          | ("80" : "eighties")
          | ("90" : "nineties")
;

